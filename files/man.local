.\" -*- nroff -*-
.\" (man.local)
.
.\" Expand line gap in japanese.
.if t \{\
.  if "\*[locale]"japanese" \{\
.am1 TH
.  nr VS (\\n[PS] * 180 / 100)
.  vs \\n[VS]u
..
.  \}
.\}
.
.\" Default heading font
.\".ds HF HB\"" use groff option: -dFH=HB
.
.\" Use headings as bookmarks.
.if t \{\
.  if !dan*bookmark \{\
.rn SH an-SH
.de SH
.  an-SH \\$@
.  pdfhref O 1 \\$*
..
.rn SS an-SS
.de SS
.  an-SS \\$@
.  pdfhref O 2 \\$*
..
.  \}
.\}
.
.\" End
